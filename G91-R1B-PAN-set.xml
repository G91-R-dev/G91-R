<PropertyList include="G91-R1B-common.xml">
    
    <sim>
        
        <description>Fiat G91 (JSBSim) PAN</description>
        
        <startup>
            <splash-texture>Aircraft/G91-R1B_HD/Splash/G91-PAN.png</splash-texture>
        </startup>
        
        <flight-model>jsb</flight-model>
        <!-- JSBSim start file -->
        <aero>G91-R1B-JSBSim</aero>
        
        <model>
            <path>Aircraft/G91-R1B_HD/Models/G91_PAN.xml</path>
        </model>
        
        <startup>
            
        </startup>
        
        <sound>
            <audible>true</audible>
            <path>Aircraft/G91-R1B_HD/Sounds/G91-sounds.xml</path>
        </sound>
        
        <menubar>
            <default>
                <menu n="10">
                    <label>Fiat G91</label>
                    <enabled type="bool">true</enabled>
                    <item>
                        <label>Test systems plane</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                                test_dialog = gui.Dialog.new("sim/gui/dialogs/G91/TestSystemsPlane/dialog","Aircraft/G91-R1B_HD/Dialogs/G91-test.xml");
                                test_dialog.open();
                            </script>
                        </binding>
                    </item>
                    <item>
                        <label>Test JATO System</label>
                        <binding>
                            <command>nasal</command>
                            <script>
                                test_dialog = gui.Dialog.new("sim/gui/dialogs/G91/TestSystemsPlane/dialog","Aircraft/G91-R1B_HD/Dialogs/G91-JATO.xml");
                                test_dialog.open();
                            </script>
                        </binding>
                    </item>
                </menu>
            </default>
        </menubar>
        
        <submodels>
            <serviceable type="bool">true</serviceable>
            <path>Aircraft/G91-R1B_HD/Models/Parts/Stores/Submodels.xml</path>
        </submodels>
        
        <configuration>
            <rudder_input_exponent type="float" archive="y">2</rudder_input_exponent>
        </configuration>
        
        <G91>
            <switchs>
                <ArmyPanels>
                    <ArmyPanelDx type="bool">false</ArmyPanelDx>
                    <ArmyPanelDx_timing type="float">0</ArmyPanelDx_timing>
                    <ArmyPanelSx type="bool">false</ArmyPanelSx>
                    <ArmyPanelSx_timing type="float">0</ArmyPanelSx_timing>
                </ArmyPanels>
                <canopy>
                    <EmergencyCanopyOpenDx type="bool">false</EmergencyCanopyOpenDx>
                    <EmergencyCanopyOpenDx_timing type="float">0</EmergencyCanopyOpenDx_timing>
                    <EmergencyCanopyOpenSx type="bool">false</EmergencyCanopyOpenSx>
                    <EmergencyCanopyOpenSx_timing type="float">0</EmergencyCanopyOpenSx_timing>
                    <handle>
                        <Emer_Canopy_Jett_Sx type="bool">false</Emer_Canopy_Jett_Sx>
                        <Emer_Canopy_Jett_Dx type="bool">false</Emer_Canopy_Jett_Dx>
                        <Emer_Canopy_Jett_Cabin type="bool">false</Emer_Canopy_Jett_Cabin>
                    </handle>
                </canopy>
            </switchs>
        </G91>
        
    </sim>
    
    <engines>
        <engine n="0">
            <running>true</running>
        </engine>
    </engines>
    
    <gear>
        <brake-parking>1</brake-parking>
    </gear>
    
    <input>
        <keyboard>
            <key n="99">
                <name>c</name>
                <desc>Canopy</desc>
                <binding>
                    <command>nasal</command>
                    <script>canopy.canopy_switch(2)</script>
                </binding>
            </key>
            <key n="4">
                <name>Ctrl-D</name>
                <desc>Drop Pylon Stores</desc>
                <binding>
                    <command>property-assign</command>
                    <property>/sim/G91/handle/handle_stores_block_flag</property>
                    <value>1</value>
                </binding>
                <binding>
                    <command>property-assign</command>
                    <property>sim/G91/stores/handleRequestToDrop</property>
                    <value>1</value>
                </binding>
            </key>
        </keyboard>
    </input>
    
    <nasal>
        <general>
            <!-- Generic -->
            <file>Aircraft/G91-R1B_HD/Nasal/Ambient/ambient_data.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Electric/electric_system_basic.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Autopilot/autopilot_set.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Engine/engine_red_emission.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Reflections/Glass_Camera_Reflections.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Reflections/Glass_Canopy_Reflections.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Lights/position_lights.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Lights/frontal_gear_lights.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Lights/re_emit_green_red_gauges.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Lights/ambient_light.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Lights/button_lights.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Lights/button_lights.nas</file>
            <file>Aircraft/G91-R1B_HD/Models/Parts/Cockpit/Switchs_Lights/Switchs_Lights.nas</file>
            <!-- Gauges -->
            <file>Aircraft/G91-R1B_HD/Models/Parts/Cockpit/Components/Gear_lever/gear_lever.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Gauges/clock.nas</file>
            <file>Aircraft/G91-R1B_HD/Nasal/Gauges/accelerometer.nas</file>
            <file>Aircraft/G91-R1B_HD/Models/Parts/Cockpit/Gauges/J8/J8.nas</file>
            <!-- Fuel -->
            <file>Aircraft/G91-R1B_HD/Nasal/Fuel/collect_tank_2_all_tank.nas</file>
            <!-- Stores -->
            <file>Aircraft/G91-R1B_HD/Nasal/Stores/Stores.nas</file>
            <!-- JATO -->
            <file>Aircraft/G91-R1B_HD/Nasal/JATO_System/JATO_System.nas</file>
        </general>
        <canvas_gauges>
            <file>Aircraft/G91-R1B_HD/Models/Parts/Canopy/Gauges/SFOM_83A/SFOM_83A_01.nas</file>
        </canvas_gauges>
        <canopy>
            <file>Aircraft/G91-R1B_HD/Nasal/Canopy/canopy.nas</file>
        </canopy>
        <tyresmoke>
            <file>Aircraft/G91-R1B_HD/Nasal/tyresmoke.nas</file>
        </tyresmoke>
    </nasal>
    
    <procedural-lights type="bool">true</procedural-lights>
    
</PropertyList>
