<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <model>
        <path>Models/Parts/Common/switch_panel_05_model.xml</path>
    </model>
    
    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>sw_north (Meshed)</object-name>
        <object-name>pin (Meshed)</object-name>
        <object-name>sw_south (Meshed)</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>fdm/jsbsim/systems/manual-switches/cockpit/sw-elevator-servo-togle</property>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>rotate</type>
        <object-name>pin (Meshed)</object-name>
        <property>fdm/jsbsim/systems/manual-switches/cockpit/sw-elevator-servo-pos</property>
        <offset-deg>15</offset-deg>
        <factor>-30</factor>
        <center>
            <x-m> 0.004</x-m>
            <y-m> 0.0</y-m>
            <z-m> 0.0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1.0</y>
            <z>0</z>
        </axis>
    </animation>
    
</PropertyList> 
 
