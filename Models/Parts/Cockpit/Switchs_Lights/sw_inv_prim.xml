<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <path>swich_panel_01.ac</path>
    
    <animation> 
        <type>material</type>
        <object-name>swich_01</object-name>
        <object-name>swich_02</object-name>
        <object-name>swich_03</object-name>
        <texture>Pictures/G91-Al_02.png</texture>
        <ambient>
            <red>0.8</red>
            <green>0.9</green>
            <blue>0.9</blue>
        </ambient>
        <emission>
            <red>0.15</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>swich_support</object-name>
        <texture>Pictures/G91-Al-dark.png</texture>
        <ambient>
            <red>0.4</red>
            <green>0.4</green>
            <blue>0.4</blue>
        </ambient>
        <specular>
            <red>0.8</red>
            <green>0.9</green>
            <blue>0.9</blue>
        </specular>
        <emission>
            <red>0.15</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>swich_04</object-name>
        <texture>Pictures/G91-Al-Light.png</texture>
        <ambient>
            <red>0.9</red>
            <green>0.9</green>
            <blue>0.9</blue>
        </ambient>
        <emission>
            <red>0.15</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Glass/glass-chrome</inherits-from>
        <object-name>swich_04</object-name>
        <object-name>swich_02</object-name>
        <object-name>swich_03</object-name>
    </effect>

    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>swich_support</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>sim/G91/switchs/cockpit/sw_inv_prim</property>
            </binding>
        </action>
    </animation>
    
     <animation>
        <type>rotate</type>
        <object-name>swich_04</object-name>
        <property>sim/G91/switchs/cockpit/sw_inv_prim</property>
        <factor>-20</factor>
        <center>
            <x-m> 0.0</x-m>
            <y-m> 0.0</y-m>
            <z-m> 0.0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
    
</PropertyList> 
 
