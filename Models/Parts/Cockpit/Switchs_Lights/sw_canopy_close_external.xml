    <animation>
        <type>material</type>
        <object-name>ring_black (Meshed)</object-name>
        <object-name>JATO_box</object-name>
        <texture>Pictures/G91-Black.png</texture>
        <ambient>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </ambient>
    </animation>
    
    <animation>
        <type>material</type>
        <object-name>cover (Meshed)</object-name>
        <texture>Pictures/G91-Al-Light.png</texture>
        <ambient>
            <red>0.7</red>
            <green>0.7</green>
            <blue>0.7</blue>
        </ambient>
    </animation>
    
    <animation>
        <type>material</type>
        <object-name>ring (Meshed)</object-name>
        <texture>Pictures/G91-Al-Light.png</texture>
        <ambient>
            <red>0.6</red>
            <green>0.6</green>
            <blue>0.6</blue>
        </ambient>
    </animation>
    
    <animation>
        <type>material</type>
        <object-name>push_button (Meshed)</object-name>
        <texture>Pictures/G91-Red_01.png</texture>
        <ambient>
            <red>0.6</red>
            <green>0.6</green>
            <blue>0.6</blue>
        </ambient>
        <specular>
            <red>1.0</red>
            <green>0.0</green>
            <blue>0.0</blue>
        </specular>
    </animation>
    
    <animation>
        <type>pick</type>
        <object-name>push_button (Meshed)</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <interval-sec>0.5</interval-sec>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/systems/manual-switches/cockpit/sw-canopy-trigger</property>
                <value>-1</value>
            </binding>
            <mod-up>
                <binding>
                    <command>property-assign</command>
                    <property>fdm/jsbsim/systems/manual-switches/cockpit/sw-canopy-trigger</property>
                    <value>0</value>
                </binding>
            </mod-up>
        </action>
    </animation>
    
    <animation>
        <type>translate</type>
        <object-name>push_button (Meshed)</object-name>
        <property>fdm/jsbsim/systems/manual-switches/external/sw-canopy-pos-close</property>
        <factor>-0.005</factor>
        <offset-m>0.0</offset-m>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    
</PropertyList>
