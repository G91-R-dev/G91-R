<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <model>
        <path>sw_model.xml</path>
    </model>

    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>sw_north</object-name>
        <object-name>sw_center</object-name>
        <object-name>sw_south</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>fdm/jsbsim/systems/electric/bus[1]/fuel-level-indicator/sw</property>
            </binding>
        </action>
    </animation>
    
     <animation>
        <type>rotate</type>
        <object-name>swich_04</object-name>
        <property>fdm/jsbsim/systems/electric/bus[1]/fuel-level-indicator/sw</property>
        <factor>-20</factor>
        <center>
            <x-m> 0.0</x-m>
            <y-m> 0.0</y-m>
            <z-m> 0.0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
    
</PropertyList> 
 
