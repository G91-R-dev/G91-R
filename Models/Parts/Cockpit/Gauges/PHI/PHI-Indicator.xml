            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- SWITCH -->

    <animation>
        <type>knob</type>
        <object-name>knob_001 (Meshed)</object-name>
        <object-name>knob_001_A (Meshed)</object-name>
        <action>
            <binding>
                <command>property-assign</command>
                <property>/fdm/jsbsim/systems/gauges/PHI/indicator/switch-turn-command</property>
                <value>1</value>
            </binding>
            <binding>
                <command>property-adjust</command>
                <property>/fdm/jsbsim/systems/gauges/PHI/indicator/switch-turn</property>
                <factor>0.25</factor>
                <min>0</min>
                <max>2</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>knob_001 (Meshed)</object-name>
        <object-name>knob_001_A (Meshed)</object-name>
        <object-name>knob_001_color (Meshed)</object-name>
        <object-name>knob_001_A_color (Meshed)</object-name>
        <property>/fdm/jsbsim/systems/gauges/PHI/indicator/switch-turned</property>
        <offset-deg>-72</offset-deg>
        <factor>72</factor>
        <center>
            <x-m>  0.004 </x-m>
            <y-m>  -0.04103 </y-m>
            <z-m>  0.04103 </z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>knob_001 (Meshed)</object-name>
        <object-name>knob_001_A (Meshed)</object-name>
        <object-name>knob_001_color (Meshed)</object-name>
        <object-name>knob_001_A_color (Meshed)</object-name>
        <property>/fdm/jsbsim/systems/gauges/PHI/indicator/switch-up</property>
        <factor>0.005</factor>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>


