    </animation>

    <!-- Gyromagnetic nord indicator -->

    <animation>
        <type>rotate</type>
        <object-name>arrow_0</object-name>
        <property>/fdm/jsbsim/systems/gauges/gyrocompass/magnetic-deg-var-linear</property>
        <axis>
