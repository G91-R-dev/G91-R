    <animation>
        <type>rotate</type>
        <object-name>frontal_plate_A</object-name>
        <property>/fdm/jsbsim/systems/gauges/PHI/attitude/gyrocompass-magnetic-deg-var-linear</property>
        <axis>
            <x> 1 </x>
            <y> 0 </y>
            <z> 0 </z>
        </axis>
    </animation>

    <model>
        <path>digit3_Indicator.xml</path>
        <offsets>
            <x-m>-0.003</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>

    <!-- VAR -->

    <animation>
        <type>knob</type>
        <object-name>knob_002 (Meshed)</object-name>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/fdm/jsbsim/systems/gauges/PHI/indicator/var-deg</property>
                <factor>1.0</factor>
                <min>-40.0</min>
                <max>40.0</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>knob_002 (Meshed)</object-name>
        <object-name>knob_002_color (Meshed)</object-name>
        <property>/fdm/jsbsim/systems/gauges/PHI/indicator/var-deg</property>
        <factor>10</factor>
        <center>
            <x-m>  0.004 </x-m>
            <y-m>  0.04103 </y-m>
            <z-m>  0.04103 </z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>knob_var (Meshed)</object-name>
        <property>/fdm/jsbsim/systems/gauges/PHI/indicator/var-deg</property>
        <factor>1.3</factor>
        <center>
            <x-m>  0.004 </x-m>
            <y-m>  0.0 </y-m>
            <z-m>  0.0 </z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- DIST -->

    <animation>
        <type>knob</type>
        <object-name>knob_004 (Meshed)</object-name>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/fdm/jsbsim/systems/gauges/PHI/indicator/dist-set</property>
                <factor>1</factor>
                <min>0.0</min>
                <max>999.0</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>knob_004 (Meshed)</object-name>
        <object-name>knob_004_color (Meshed)</object-name>
        <property>/fdm/jsbsim/systems/gauges/PHI/indicator/dist-set</property>
        <factor>10</factor>
        <center>
            <x-m>  0.004 </x-m>
            <y-m>  -0.041 </y-m>
            <z-m>  -0.042 </z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- BRG -->

    <animation>
        <type>knob</type>
        <object-name>knob_003 (Meshed)</object-name>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/fdm/jsbsim/systems/gauges/PHI/indicator/brg-imposed</property>
                <factor>1</factor>
                <min>0.0</min>
                <max>359.0</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>knob_003 (Meshed)</object-name>
        <object-name>knob_003_color (Meshed)</object-name>
        <property>/fdm/jsbsim/systems/gauges/PHI/indicator/brg-imposed</property>
        <factor>-5</factor>
        <center>
            <x-m>  0.004 </x-m>
            <y-m>  0.041 </y-m>
            <z-m>  -0.042 </z-m>
        </center>
