    <!-- animate needles -->
    
    <animation>
        <type>rotate</type>
        <object-name>Arrow_3A (Meshed)</object-name>
        <object-name>Arrow_3B (Meshed)</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
        <factor>0.00833333333</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Arrow_4A (Meshed)</object-name>
        <object-name>Arrow_4B (Meshed)</object-name>
        <property>/instrumentation/clock/indicated-sec</property>
        <factor>0.1</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>Arrow_1A (Meshed)</object-name>
        <object-name>Arrow_1B (Meshed)</object-name>
        <property>/fdm/jsbsim/systems/gauges/clock/seconds-lag</property>
        <factor>6.0</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    
    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>knob_002 (Meshed)</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-cycle</command>
                <property>/fdm/jsbsim/systems/gauges/clock/push-status</property>
                <value type="double">1</value>
                <value type="double">2</value>
                <value type="double">0</value>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>rotate</type>
        <object-name>Arrow_2A (Meshed)</object-name>
        <object-name>Arrow_2B (Meshed)</object-name>
        <property>/fdm/jsbsim/systems/gauges/clock/minutes-lag</property>
        <factor>6</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>knob_001 (Meshed)</object-name>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/instrumentation/clock/offset-sec</property>
                <factor>60</factor>
                <min>-86400</min>
                <max>86400</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    
    <model>
        <name>procedural_light_cockpit_gauge_spot_red</name>
        <path>Aircraft/G91-R1B_HD/Models/Parts/Cockpit/Effects/lights/procedural_light_cockpit_gauge_spot_red.xml</path>
        <offsets>
            <x-m> 0.03</x-m>
            <y-m> 0.0 </y-m>
            <z-m> 0.0 </z-m>
        </offsets>
    </model>
    
