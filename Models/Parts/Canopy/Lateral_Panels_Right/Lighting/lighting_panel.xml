<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <path>lighting_panel.ac</path>
    
    <animation>
        <type>noshadow</type>
        <object-name>frontal support back (Meshed)</object-name>
        <object-name>frontal support B (Meshed)</object-name>
        <object-name>frontal support A (Meshed)</object-name>
        <object-name>instrument lights B (Meshed)</object-name>
        <object-name>instrument lights A (Meshed)</object-name>
        <object-name>console lights B (Meshed)</object-name>
        <object-name>console lights A (Meshed)</object-name>
        <object-name>console panel lights B (Meshed)</object-name>
        <object-name>console panel lights A (Meshed)</object-name>
        <object-name>frontal screews (Meshed)</object-name>
        <object-name>screews (Meshed)</object-name>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>frontal support back (Meshed)</object-name>
        <diffuse>
            <red>0.1</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </diffuse>
        <ambient>
            <red>0.05</red>
            <green>0.05</green>
            <blue>0.05</blue>
        </ambient>
        <specular>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </specular>
        <emission>
            <red>0.08</red>
            <green>0.0</green>
            <blue>0.02</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>frontal support B (Meshed)</object-name>
        <diffuse>
            <red>0.1</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </diffuse>
        <ambient>
            <red>0.1</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </ambient>
        <specular>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </specular>
        <emission>
            <red>0.14</red>
            <green>0.0</green>
            <blue>0.02</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>frontal support A (Meshed)</object-name>
        <diffuse>
            <red>0.28</red>
            <green>0.28</green>
            <blue>0.28</blue>
        </diffuse>
        <ambient>
            <red>1.0</red>
            <green>0.9</green>
            <blue>0.6</blue>
        </ambient>
        <emission>
            <red>0.25</red>
            <green>0.0</green>
            <blue>0.02</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>console lights B (Meshed)</object-name>
        <object-name>console panel lights B (Meshed)</object-name>
        <object-name>instrument lights B (Meshed)</object-name>
        <diffuse>
            <red>0.1</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </diffuse>
        <ambient>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </ambient>
        <specular>
            <red>0.3</red>
            <green>0.3</green>
            <blue>0.3</blue>
        </specular>
        <emission>
            <red>0.08</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>console lights A (Meshed)</object-name>
        <object-name>console panel lights A (Meshed)</object-name>
        <object-name>instrument lights A (Meshed)</object-name>
        <diffuse>
            <red>0.28</red>
            <green>0.28</green>
            <blue>0.28</blue>
        </diffuse>
        <diffuse>
            <red>0.5</red>
            <green>0.5</green>
            <blue>0.5</blue>
        </diffuse>
        <ambient>
            <red>1.0</red>
            <green>0.9</green>
            <blue>0.6</blue>
        </ambient>
        <emission>
            <red>0.24</red>
            <green>0.08</green>
            <blue>0.02</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>frontal screews (Meshed)</object-name>
        <diffuse>
            <red>0.23</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </diffuse>
        <ambient>
            <red>0.35</red>
            <green>0.3</green>
            <blue>0.3</blue>
        </ambient>
        <specular>
            <red>0.9</red>
            <green>0.9</green>
            <blue>0.9</blue>
        </specular>
        <emission>
            <red>0.18</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>screews (Meshed)</object-name>
        <diffuse>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </diffuse>
        <ambient>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </ambient>
        <specular>
            <red>0.5</red>
            <green>0.5</green>
            <blue>0.5</blue>
        </specular>
        <emission>
            <red>0.12</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>instrument lights A (Meshed)</object-name>
        <object-name>instrument lights B (Meshed)</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-adjust</command>
                <property>fdm/jsbsim/systems/lightning/light-uv-instrument-bright</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>                
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>rotate</type>
        <object-name>instrument lights A (Meshed)</object-name>
        <object-name>instrument lights B (Meshed)</object-name>
        <property>fdm/jsbsim/systems/lightning/light-uv-instrument-bright-pos</property>
        <factor>210</factor>
        <offset-deg>-120.0</offset-deg>
        <center>
            <x-m>-0.127</x-m>
            <y-m>-0.026</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>console panel lights A (Meshed)</object-name>
        <object-name>console panel lights B (Meshed)</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-adjust</command>
                <property>fdm/jsbsim/systems/lightning/light-red-emer-instrument-bright</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>                
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>rotate</type>
        <object-name>console panel lights A (Meshed)</object-name>
        <object-name>console panel lights B (Meshed)</object-name>
        <property>fdm/jsbsim/systems/lightning/light-red-emer-instrument-bright-pos</property>
        <factor>210</factor>
        <offset-deg>-120.0</offset-deg>
        <center>
            <x-m>-0.079</x-m>
            <y-m>-0.024</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>console lights A (Meshed)</object-name>
        <object-name>console lights B (Meshed)</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-adjust</command>
                <property>fdm/jsbsim/systems/lightning/light-red-console-bright</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>                
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>rotate</type>
        <object-name>console lights A (Meshed)</object-name>
        <object-name>console lights B (Meshed)</object-name>
        <property>fdm/jsbsim/systems/lightning/light-red-console-bright-pos</property>
        <factor>210</factor>
        <offset-deg>-120.0</offset-deg>
        <center>
            <x-m>-0.028</x-m>
            <y-m>-0.024</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
    </animation>
    
    <model>
        <path>flasher.xml</path>
        <offsets>
            <x-m>-0.236</x-m>
            <y-m>-0.033</y-m>
            <z-m>0.005</z-m>
            <roll-deg>180.0</roll-deg>
            <pitch-deg>90</pitch-deg>
            <heading-deg>180.0</heading-deg>
        </offsets>
    </model>
    
    <model>
        <path>position_lights.xml</path>
        <offsets>
            <x-m>-0.180</x-m>
            <y-m>-0.033</y-m>
            <z-m>0.005</z-m>
            <roll-deg>180.0</roll-deg>
            <pitch-deg>90</pitch-deg>
            <heading-deg>180.0</heading-deg>
        </offsets>
    </model>
    
    <model>
        <path>anticollision_lights.xml</path>
        <offsets>
            <x-m>-0.026</x-m>
            <y-m>0.014</y-m>
            <z-m>0.005</z-m>
            <roll-deg>180.0</roll-deg>
            <pitch-deg>90</pitch-deg>
            <heading-deg>180.0</heading-deg>
        </offsets>
    </model>
    
</PropertyList>
