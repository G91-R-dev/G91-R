<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <path>breaker_panel.ac</path>
    
    <animation>
        <type>noshadow</type>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Interior_Shading/Interior_Shading</inherits-from>
