        <ambient>
            <red>0.35</red>
            <green>0.35</green>
            <blue>0.35</blue>
        </ambient>
        <specular>
            <red>0.3</red>
            <green>0.3</green>
            <blue>0.3</blue>
        </specular>
        <emission>
            <red>0.12</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>base top (Meshed)</object-name>
        <transparency>
            <alpha>0.8</alpha>
        </transparency>
        <diffuse>
            <red>0.61</red>
            <green>0.65</green>
            <blue>0.67</blue>
