<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <path>doppler_ground_test.ac</path>
    
    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Interior_Shading/Interior_Shading</inherits-from>
        <object-name>square (Meshed)</object-name>
        <object-name>text A (Meshed)</object-name>
    </effect>
    
    <animation> 
        <type>material</type>
        <object-name>square (Meshed)</object-name>
        <diffuse>
            <red>0.4</red>
            <green>0.4</green>
            <blue>0.4</blue>
        </diffuse>
        <ambient>
            <red>0.2</red>
            <green>0.2</green>
            <blue>0.2</blue>
        </ambient>
        <specular>
            <red>0.4</red>
            <green>0.4</green>
            <blue>0.4</blue>
        </specular>
        <emission>
            <red>0.14</red>
            <green>0.0</green>
            <blue>0.01</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>text A (Meshed)</object-name>
        <diffuse>
            <red>0.28</red>
            <green>0.28</green>
            <blue>0.28</blue>
        </diffuse>
        <ambient>
            <red>1.0</red>
            <green>0.9</green>
            <blue>0.6</blue>
        </ambient>
        <emission>
            <red>0.2</red>
            <green>0.0</green>
            <blue>0.01</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>
    
    <model>
        <path>sw_doppler_ground_text.xml</path>
        <offsets>
            <x-m> 0.045</x-m>
            <y-m>-0.009</y-m>
            <z-m>0.0055</z-m>
            <roll-deg>180.0</roll-deg>
            <pitch-deg>180.0</pitch-deg>
            <heading-deg>0.0</heading-deg>
        </offsets>
    </model>
    
</PropertyList>
