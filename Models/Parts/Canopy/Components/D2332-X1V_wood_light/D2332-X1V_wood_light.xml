    <!-- Compositor UV lights -->
    
    <light>
        <name>canopy-uv-sx</name>
        <type>spot</type>
        <position>
            <x-m>-2.8</x-m>
            <y-m>0.25</y-m>
            <z-m>0.30</z-m>
        </position>
        <direction>
            <pitch-deg>60</pitch-deg>
            <roll-deg>0</roll-deg>
            <heading-deg>6</heading-deg>
        </direction>
        <ambient>
            <r>0.0</r>
            <g>0.0</g>
            <b>0.0</b>
            <a>1</a>
        </ambient>
        <diffuse>
            <r>0.1</r>
            <g>0.0</g>
            <b>0.3</b>
            <a>1</a>
        </diffuse>
        <specular>
            <r>0.4</r>
