    <animation>
        <type>select</type>
        <object-name>canopy-uv-dx</object-name>
        <object-name>canopy-uv-sx</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>fdm/jsbsim/systems/lightning/light-uv-instrument</property>
                    <value type="double">0.01</value>
                </greater-than>
                <equals>
                    <property>sim/current-view/internal</property>
                    <value>true</value>
                </equals>
            </and>
        </condition>
    </animation>
    
    <!-- animation -->

    <animation> 
        <type>rotate</type>
        <object-name>CanopyGroup</object-name>
        <object-name>canopy-uv-dx</object-name>
        <object-name>canopy-uv-sx</object-name>
        <property>fdm/jsbsim/systems/canopy/position</property>
        <factor>45</factor>
        <center>
            <x-m>-1.50104</x-m>
            <y-m> 0.0</y-m>
            <z-m> 0.55355</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
    
