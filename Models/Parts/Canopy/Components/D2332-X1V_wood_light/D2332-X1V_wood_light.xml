            <b>1.8</b>
            <a>1</a>
        </specular>
        <attenuation>
            <c>1.0</c>
            <l>0.3</l>
            <q>2.0</q>
        </attenuation>
        <spot-exponent>20</spot-exponent>
        <spot-cutoff>70</spot-cutoff>
        <range-m>2</range-m>
        <dim-factor>
            <property>fdm/jsbsim/systems/lightning/light-uv-instrument</property>
