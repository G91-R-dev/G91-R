<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <path>knob_type_01.ac</path>

    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Reflections/reflection_black_00</inherits-from>
        <object-name>Swicht_gun_01</object-name>
    </effect>

    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Interior_Shading/Interior_Shading</inherits-from>
        <object-name>Swicht_gun_01</object-name>
    </effect>
    
    <animation> 
        <type>material</type>
        <object-name>Swicht_gun_01</object-name>
        <texture>Pictures/G91-Dark-Grey.png</texture>
        <ambient>
            <red>0.6</red>
            <green>0.6</green>
            <blue>0.6</blue>
        </ambient>
        <emission>
            <red>0.04</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>Swich_gun_02</object-name>
        <texture>Pictures/G91-Al-Light.png</texture>
        <ambient>
            <red>1.0</red>
            <green>0.9</green>
            <blue>0.6</blue>
        </ambient>
        <emission>
            <red>0.02</red>
            <green>0.1</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_phosphorescent_light</factor-prop>
        </emission>
    </animation>
    
    <animation>
        <type>rotate</type>
        <object-name>Swicht_gun_01</object-name>
        <object-name>Swich_gun_02</object-name>
        <property>fdm/jsbsim/systems/electric/bus[1]/collimator-lighting/sw</property>
        <factor>-180</factor>
        <offset-deg>53</offset-deg>
        <center>
            <x>0.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    
    <animation>
        <type>knob</type>
        <object-name>Swicht_gun_01</object-name>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>fdm/jsbsim/systems/electric/bus[1]/collimator-lighting/sw</property>
                <factor>0.1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>  
    
    <model>
        <name>procedural_light_cockpit_gauge_spot_red</name>
        <path>Aircraft/G91-R1B_HD/Models/Parts/Cockpit/Effects/lights/procedural_light_cockpit_gauge_spot_red.xml</path>
        <offsets>
            <x-m> 0.03</x-m>
            <y-m> 0.0 </y-m>
            <z-m> 0.0 </z-m>
        </offsets>
    </model>
    
</PropertyList>
