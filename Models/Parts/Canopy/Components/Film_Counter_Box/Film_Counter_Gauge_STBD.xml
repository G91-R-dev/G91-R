<PropertyList>
    
    <model>
        <path>Film_Counter_Gauge_model.xml</path>
    </model>
    
    <!--
    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Reflections/reflection_black_00</inherits-from>
        <object-name>forward (Meshed)</object-name>
        <object-name>port (Meshed)</object-name>
        <object-name>vertic (Meshed)</object-name>
        <object-name>stbd (Meshed)</object-name>
    </effect>
    -->
    
    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Interior_Shading/Interior_Shading</inherits-from>
        <object-name>forward (Meshed)</object-name>
        <object-name>port (Meshed)</object-name>
        <object-name>vertic (Meshed)</object-name>
        <object-name>stbd (Meshed)</object-name>
    </effect>
    
    <animation>
        <type>select</type>
        <name>not_display_legend</name>
        <object-name>forward (Meshed)</object-name>
        <object-name>port (Meshed)</object-name>
        <object-name>vertic (Meshed)</object-name>
        <condition>
            <equals>
                <property>sim/G91/aux/true</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>stbd (Meshed)</object-name>
        <diffuse>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.7</blue>
        </diffuse>
        <ambient>
            <red>0.5</red>
            <green>0.5</green>
            <blue>0.4</blue>
        </ambient>
        <specular>
            <red>0.3</red>
            <green>0.3</green>
            <blue>0.3</blue>
        </specular>
        <emission>
            <red-prop>sim/G91/re_emit/gauge_red_type_b_phosphorescent_light</red-prop>
            <green-prop>sim/G91/re_emit/gauge_phosphorescent_light_type_b</green-prop>
            <blue>0.0</blue>
            <factor>0.10</factor>
        </emission>
    </animation>
    
</PropertyList>

