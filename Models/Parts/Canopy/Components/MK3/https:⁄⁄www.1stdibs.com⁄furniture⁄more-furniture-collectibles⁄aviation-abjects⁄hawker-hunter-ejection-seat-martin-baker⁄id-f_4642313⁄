 their novels.\r\n\r\n","priceUI":{"type_tracking":"list price","converted_amounts":{"CHF":"16,780 CHF","EUR":"\u20ac14.377","USD":"$17,037","MXN":"$315,915 MXN","GBP":"\u00a312,500","CAD":"$21,843 CAD","AUD":"$22,461 AUD"},"states":{"ctas":{"show_request_price":false,"show_purchase_button":true,"show_make_offer":true},"price_displays":{"show_sale_price":false,"show_net_price":false,"show_retail_price":true,"show_price_upon_request":false,"show_net_price_label":false,"show_sale_price_label":false,"show_list_price_label":false,"show_on_hold":false,"show_percent_off":false,"show_reduced_from":false,"show_sold":false,"show_label":false,"show_not_listed":false,"is_unavailable":false}},"original_amounts":null,"cta_urls":{"request_price":"","purchase":"\/checkout\/id-f_5053703","make_offer":"\/checkout\/id-f_5053703"},"usd_price":17037.78,"formatted_pdp_amounts":{"retail_price":17037.78},"discount_percentage":null},"is_sale":false,"location":"London, GB, SW10 0TZ"},{"creators_by":"","id":"f_9010321","integerId":"9010321","title":"Arts and Crafts Glockenspiel by Plant & Perry, circa 1890","nth":0,"vertical":"furniture","categories":[],"image":"https:\/\/a.1stdibscdn.com\/archivesE\/upload\/1121189\/f_90103211510234213615\/9010321_master.jpg?width=240","uri":"\/furniture\/more-furniture-collectibles\/collectibles-curiosities\/musical-instruments\/arts-crafts-glockenspiel-plant-perry-circa-1890\/id-f_9010321\/","sold":false,"on_hold":false,"price":{"amounts":{"CHF":"1,678 CHF","EUR":"\u20ac1.437","USD":"$1,703","MXN":"$31,591 MXN","GBP":"\u00a31,250","CAD":"$2,184 CAD","AUD":"$2,246 AUD"},"initial_amounts":null},"shipping":null,"seller":{"company":"Hatchwell Antiques","logo":"\/f_11902\/DEALER_PDP_LOGO\/1455720228873\/7mjppm7e\/f_11902_pdp1.png","uri":"\/dealers\/hatchwell-antiques\/","id":"f_11902","address":"London, GB"},"measurements":{"size":null,"diameter":null,"width":null,"height":null,"length":null,"depth":null,"display":"","label":"Dimensions","values":""},"creators