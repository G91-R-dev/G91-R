<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <path>handle_brake.ac</path>
    
    <offsets>
        <x-m>0.0</x-m>
        <y-m>0.0</y-m>
        <z-m>0.0</z-m>
    </offsets>
    
    <effect>
        <inherits-from>Emer_Canopy_Jett</inherits-from>
        <object-name>handle_cockpit02.000</object-name>
    </effect>
    
    <!--
    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Reflections/reflection_black_00</inherits-from>
        <object-name>handle_cockpit02.003</object-name>
        <object-name>handle_cockpit02.001</object-name>
        <object-name>wire</object-name>
    </effect>
    -->
    
    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Interior_Shading/Interior_Shading</inherits-from>
        <object-name>handle_cockpit02.003</object-name>
        <object-name>handle_cockpit02.001</object-name>
    </effect>
    
    <animation> 
        <type>material</type>
        <object-name>handle_cockpit02.000</object-name>
        <ambient>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </ambient>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>wire</object-name>
        <texture>Pictures/G91-Dark-Grey.png</texture>
        <ambient>
            <red>0.6</red>
            <green>0.6</green>
            <blue>0.6</blue>
        </ambient>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>handle_cockpit02.003</object-name>
        <object-name>handle_cockpit02.001</object-name>
        <texture>Pictures/G91-Al.png</texture>
        <ambient>
            <red>0.6</red>
            <green>0.6</green>
            <blue>0.6</blue>
        </ambient>
    </animation>
    
    <animation>
        <type>pick</type>
        <object-name>handle_cockpit02.000</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-toggle</command>
                <property>sim/G91/switchs/canopy/handle/Emer_Canopy_Jett_Dx</property>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>translate</type>
        <object-name>handle_cockpit02.000</object-name>
        <object-name>handle_cockpit02.001</object-name>
        <object-name>handle_cockpit02.003</object-name>
        <object-name>wire</object-name>
        <property>sim/G91/switchs/canopy/handle/Emer_Canopy_Jett_Dx</property>
        <factor>0.1</factor>
        <offset-m>0.0</offset-m>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

</PropertyList>
