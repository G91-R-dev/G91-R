<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <path>JATO_Ready_Switch.ac</path>

    <!--
    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Reflections/reflection_black_00</inherits-from>
        <object-name>cover (Meshed)</object-name>
        <object-name>frontal (Meshed)</object-name>
        <object-name>protection (Meshed)</object-name>
        <object-name>protection_A (Meshed)</object-name>
        <object-name>screewsD6 (Meshed)</object-name>
        <object-name>screewsD7 (Meshed)</object-name>
        <object-name>support (Meshed)</object-name>
        <object-name>switch (Meshed)</object-name>
    </effect>
    -->
    
    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Interior_Shading/Interior_Shading</inherits-from>
        <object-name>cover (Meshed)</object-name>
        <object-name>frontal (Meshed)</object-name>
        <object-name>protection (Meshed)</object-name>
        <object-name>protection_A (Meshed)</object-name>
        <object-name>screewsD6 (Meshed)</object-name>
        <object-name>screewsD7 (Meshed)</object-name>
        <object-name>support (Meshed)</object-name>
        <object-name>switch (Meshed)</object-name>
    </effect>
    
    <animation> 
        <type>material</type>
        <object-name>support (Meshed)</object-name>
        <texture>Pictures/G91-light-blue-01.png</texture>
        <ambient>
            <red>0.5</red>
            <green>0.5</green>
            <blue>0.5</blue>
        </ambient>
        <emission>
            <red>0.051</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>cover (Meshed)</object-name>
        <texture>Pictures/G91-Al-Light.png</texture>
        <ambient>
            <red>0.6</red>
            <green>0.6</green>
            <blue>0.6</blue>
        </ambient>
        <emission>
            <red>0.051</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>switch (Meshed)</object-name>
        <texture>Pictures/G91-Al-01.png</texture>
        <ambient>
            <red>0.6</red>
            <green>0.6</green>
            <blue>0.6</blue>
        </ambient>
        <emission>
            <red>0.051</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>screewsD6 (Meshed)</object-name>
        <object-name>screewsD7 (Meshed)</object-name>
        <texture>Pictures/G91-Dark-Grey.png</texture>
        <ambient>
            <red>0.3</red>
            <green>0.3</green>
            <blue>0.3</blue>
        </ambient>
        <emission>
            <red>0.03</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>protection (Meshed)</object-name>
        <object-name>frontal (Meshed)</object-name>
        <texture>Pictures/G91-Dark-Grey.png</texture>
        <ambient>
            <red>0.3</red>
            <green>0.3</green>
            <blue>0.3</blue>
        </ambient>
        <emission>
            <red>0.03</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>protection_A (Meshed)</object-name>
        <transparency>
            <alpha>0.0</alpha>
        </transparency>
        <ambient>
            <red>0.80</red>
            <green>0.73</green>
            <blue>0.53</blue>
        </ambient>
        <emission>
            <red>0.045</red>
            <green>0.15</green>
            <blue>0.031</blue>
            <factor-prop>sim/G91/re_emit/gauge_phosphorescent_light</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>switch (Meshed)</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>fdm/jsbsim/systems/manual-switches/jato/sw-ready-togle</property>
            </binding>
        </action>
    </animation>
    
    <animation>
        <type>rotate</type>
        <object-name>switch (Meshed)</object-name>
        <property>fdm/jsbsim/systems/manual-switches/jato/sw-ready-pos</property>
        <factor>-80</factor>
        <offset-deg>40</offset-deg>
        <center>
            <x-m> 0.0</x-m>
            <y-m> -0.005</y-m>
            <z-m> -0.007</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
    
</PropertyList>
