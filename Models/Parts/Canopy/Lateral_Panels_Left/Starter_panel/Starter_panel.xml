        <object-name>panel_start01</object-name>
        <diffuse>
            <red>0.7</red>
            <green>0.7</green>
            <blue>0.7</blue>
        </diffuse>
        <ambient>
            <red>0.6</red>
            <green>0.6</green>
            <blue>0.6</blue>
        </ambient>
        <emission>
            <red>0.07</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_emer_30_red_console</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>panel_start2</object-name>
