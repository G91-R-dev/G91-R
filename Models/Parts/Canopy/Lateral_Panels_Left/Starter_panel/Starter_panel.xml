<PropertyList>
    
    <path>panel_start.ac</path>
    
    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Reflections/reflection_black_00</inherits-from>
        <object-name>panel_start01</object-name>
        <object-name>panel_start2</object-name>
        <object-name>panel_start_tex</object-name>
    </effect>
    
    <effect>
        <inherits-from>Aircraft/G91-R1B_HD/Models/Effects/Interior_Shading/Interior_Shading</inherits-from>
        <object-name>panel_start01</object-name>
        <object-name>panel_start2</object-name>
        <object-name>panel_start_tex</object-name>
    </effect>
    
    <animation> 
        <type>material</type>
        <object-name>panel_start01</object-name>
        <texture>Pictures/G91-Internal-Gray.png</texture>
        <ambient>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </ambient>
        <emission>
            <red>0.07</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>panel_start2</object-name>
        <texture>Pictures/G91-Dark-Grey.png</texture>
        <ambient>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </ambient>
        <specular>
            <red>0.98</red>
            <green>0.92</green>
            <blue>0.84</blue>
        </specular>
        <emission>
            <red>0.15</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>panel_start_tex</object-name>
        <transparency>
            <alpha>1.0</alpha>
        </transparency>
        <ambient>
            <red>1.0</red>
            <green>0.9</green>
            <blue>0.6</blue>
        </ambient>
        <emission>
            <red>0.2</red>
            <green>0.15</green>
            <blue>0.12</blue>
            <factor-prop>sim/G91/re_emit/gauge_red_light</factor-prop>
        </emission>
    </animation>
    
    <model>
