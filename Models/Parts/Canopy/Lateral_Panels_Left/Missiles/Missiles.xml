<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

    <path>missiles.ac</path>

    <animation>
        <type>noshadow</type>
        <object-name>support (Meshed)</object-name>
        <object-name>support A (Meshed)</object-name>
        <object-name>screews (Meshed)</object-name>
    </animation>

    <animation>
        <type>material</type>
        <object-name>support (Meshed)</object-name>
        <diffuse>
            <red>0.1</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </diffuse>
        <ambient>
            <red>0.1</red>
            <green>0.1</green>
            <blue>0.1</blue>
        </ambient>
        <emission>
            <red>0.14</red>
            <green>0.0</green>
            <blue>0.02</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>screews (Meshed)</object-name>
        <diffuse>
            <red>0.35</red>
            <green>0.35</green>
            <blue>0.35</blue>
        </diffuse>
        <ambient>
            <red>0.35</red>
            <green>0.35</green>
            <blue>0.35</blue>
        </ambient>
        <specular>
            <red>1.0</red>
            <green>1.0</green>
            <blue>1.0</blue>
        </specular>
        <emission>
            <red>0.1</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor-prop>sim/G91/re_emit/light_red_console</factor-prop>
        </emission>
    </animation>

    <animation>
        <type>material</type>
        <object-name>support A (Meshed)</object-name>
        <diffuse>
            <red>0.6</red>
            <green>0.6</green>
            <blue>0.5</blue>
        </diffuse>
        <ambient>
            <red>0.6</red>
            <green>0.6</green>
            <blue>0.5</blue>
        </ambient>
        <emission>
            <red>0.16</red>
            <green>0.09</green>
            <blue>0.04</blue>
            <factor-prop>fdm/jsbsim/systems/lightning/light-red-console</factor-prop>
        </emission>
    </animation>

    <model>
        <path>lg_blue.xml</path>
        <offsets>
            <x-m>0.072</x-m>
            <y-m>-0.093</y-m>
            <z-m>0.013</z-m>
            <pitch-deg>-90.0</pitch-deg>
        </offsets>
    </model>

    <model>
        <path>sw_missiles_selector.xml</path>
        <offsets>
            <x-m>0.072</x-m>
            <y-m>-0.228</y-m>
            <z-m>0.004</z-m>
            <roll-deg>180.0</roll-deg>
            <pitch-deg>90.0</pitch-deg>
            <heading-deg>180.0</heading-deg>
        </offsets>
    </model>

    <model>
        <path>sw_missiles_ready.xml</path>
        <offsets>
            <x-m>0.072</x-m>
            <y-m>-0.183</y-m>
            <z-m>0.005</z-m>
            <roll-deg>180.0</roll-deg>
            <pitch-deg>180.0</pitch-deg>
            <heading-deg>0.0</heading-deg>
        </offsets>
    </model>

    <model>
        <path>sw_power_supply.xml</path>
        <offsets>
            <x-m>0.072</x-m>
            <y-m>-0.138</y-m>
            <z-m>0.004</z-m>
            <roll-deg>180.0</roll-deg>
            <pitch-deg>90.0</pitch-deg>
            <heading-deg>180.0</heading-deg>
        </offsets>
