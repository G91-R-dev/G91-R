            <property>fdm/jsbsim/systems/lightning/light-red-console</property>
        </dim-factor>
    </light>

    <animation>
        <type>select</type>
        <object-name>canopy-Consolle-red-sx-01</object-name>
        <object-name>canopy-Consolle-red-sx-02</object-name>
        <object-name>canopy-Consolle-red-sx-03</object-name>
        <object-name>canopy-Consolle-red-dx-01</object-name>
        <object-name>canopy-Consolle-red-dx-02</object-name>
        <object-name>canopy-Consolle-red-dx-03</object-name>
        <object-name>canopy-ambient-red</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>fdm/jsbsim/systems/lightning/light-red-console</property>
                    <value type="double">0.01</value>
                </greater-than>
                <equals>
                    <property>sim/current-view/internal</property>
                    <value>true</value>
                </equals>
            </and>
        </condition>
    </animation>

    <!-- Compositor anticollision light -->

    <light>
        <name>anticollision-top-spot</name>
        <type>spot</type>
        <position>
            <x-m>-0.55</x-m>
            <y-m>0.0</y-m>
            <z-m>0.45</z-m>
        </position>
        <direction>
            <pitch-deg>195.0</pitch-deg>
            <roll-deg>0.0</roll-deg>
            <heading-deg>0.0</heading-deg>
        </direction>
        <ambient>
            <r>0.3</r>
            <g>0.3</g>
            <b>0.3</b>
            <a>1</a>
        </ambient>
        <diffuse>
            <r>3.0</r>
            <g>3.0</g>
            <b>2.0</b>
            <a>1</a>
        </diffuse>
        <specular>
            <r>0.3</r>
            <g>0.3</g>
            <b>0.3</b>
            <a>1</a>
        </specular>
        <attenuation>
            <c>1.0</c>
            <l>0.01</l>
            <q>0.005</q>
        </attenuation>
        <spot-exponent>2</spot-exponent>
        <spot-cutoff>80</spot-cutoff>
        <range-m>50.0</range-m>
        <dim-factor>
            <property>fdm/jsbsim/systems/lightning/ambient-light-anticollision-intensity</property>
        </dim-factor>
    </light>

    <animation>
        <type>select</type>
        <object-name>anticollision-top-spot</object-name>
        <object-name>canopy-ambient-red</object-name>
        <condition>
            <greater-than>
                <property>fdm/jsbsim/systems/lightning/ambient-light-anticollision-intensity</property>
                <value type="double">0.01</value>
            </greater-than>
        </condition>
    </animation>

    <!-- Ambient day gray light correction -->

    <light>
        <name>canopy-ambient-gray</name>
        <type>point</type>
        <position>
            <x-m>-2.7</x-m>
            <y-m>0.0</y-m>
            <z-m>1.2</z-m>
        </position>
        <direction>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </direction>
        <ambient>
            <r>0.01</r>
            <g>0.01</g>
            <b>0.01</b>
            <a>1</a>
        </ambient>
        <diffuse>
            <r>0.1</r>
            <g>0.1</g>
            <b>0.1</b>
            <a>1</a>
        </diffuse>
        <specular>
            <r>0.6</r>
            <g>0.6</g>
            <b>0.6</b>
            <a>1</a>
        </specular>
        <attenuation>
            <c>1.0</c>
            <l>0.09</l>
            <q>0.032</q>
        </attenuation>
        <spot-exponent>5</spot-exponent>
        <spot-cutoff>50</spot-cutoff>
        <range-m>2</range-m>
        <dim-factor>
            <expression>
                <product>
                    <property>sim/G91/ambient-data/irradiance_light_green</property>
                    <property>sim/G91/configuration/compositor/canopy-ambient-light-dimmer</property>
                </product>
            </expression>
        </dim-factor>
    </light>


    <animation>
        <type>select</type>
        <object-name>canopy-ambient-gray</object-name>
        <condition>
            <and>
                <greater-than>
                    <expression>
                        <product>
                            <property>sim/G91/configuration/compositor/canopy-ambient-light-dimmer</property>
                            <property>sim/G91/ambient-data/irradiance_light_green</property>
                        </product>
                    </expression>
                    <value type="double">0.01</value>
                </greater-than>
                <equals>
                    <property>sim/current-view/internal</property>
                    <value>true</value>
                </equals>
            </and>
        </condition>
    </animation>

    <light>
        <name>canopy-ambient-gray-moon</name>
        <type>point</type>
        <position>
            <x-m>-2.7</x-m>
            <y-m>0.0</y-m>
            <z-m>1.2</z-m>
        </position>
        <direction>
            <x>-1.0</x>
            <y>0.0</y>
            <z>0.0</z>
        </direction>
        <ambient>
            <r>0.0</r>
            <g>0.0</g>
            <b>0.0</b>
            <a>1</a>
        </ambient>
        <diffuse>
            <r>0.1</r>
            <g>0.1</g>
            <b>0.1</b>
            <a>1</a>
        </diffuse>
        <specular>
            <r>0.4</r>
            <g>0.4</g>
            <b>0.45</b>
            <a>1</a>
        </specular>
        <attenuation>
            <c>1.0</c>
            <l>0.09</l>
            <q>0.032</q>
        </attenuation>
        <spot-exponent>20</spot-exponent>
        <spot-cutoff>50</spot-cutoff>
        <range-m>2</range-m>
        <dim-factor>
            <expression>
                <product>
                    <property>ephemeris/moon/phase</property>
                    <cos><property>sim/G91/ambient-data/irradiance_light_green</property></cos>
                    <property>sim/G91/configuration/compositor/canopy-ambient-gray-specular-dimmer</property>
                </product>
            </expression>
        </dim-factor>
    </light>

    <animation>
        <type>select</type>
        <object-name>canopy-ambient-gray-moon</object-name>
        <condition>
            <and>
                <lower-than>
                    <expression>
                        <product>
                            <property>sim/G91/configuration/compositor/canopy-ambient-light-dimmer</property>
                            <property>sim/G91/ambient-data/irradiance_light_green</property>
                        </product>
                    </expression>
                    <value type="double">0.01</value>
                </lower-than>
                <equals>
                    <property>sim/current-view/internal</property>
                    <value>true</value>
                </equals>
            </and>
        </condition>
    </animation>

</PropertyList>
