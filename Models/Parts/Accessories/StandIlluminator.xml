    <animation>
        <type>blend</type>
        <property>sim/G91/glass_camera_reflections</property>
        <factor>0.2</factor>
        <object-name>lamp cone internal (Meshed)</object-name>
    </animation>

    <!-- Animations -->

    <animation>
        <type>knob</type>
        <object-name>pipe top (Meshed)</object-name>
        <object-name>plate top (Meshed)</object-name>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/G91/accessories/standIlluminators/yaw</property>
                <factor>3.0</factor>
                <min>0</min>
                <max>360</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>support top (Meshed)</object-name>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/sim/G91/accessories/standIlluminators/pitch</property>
                <factor>3.0</factor>
                <min>-10.0</min>
                <max>30.0</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <object-name>Head</object-name>
        <object-name>support top (Meshed)</object-name>
        <object-name>StandIlluminator-spot-01</object-name>
        <object-name>StandIlluminator-spot-02</object-name>
        <type>rotate</type>
        <property>sim/G91/accessories/standIlluminators/yaw</property>
        <factor>1.0</factor>
        <offset-deg>0.0</offset-deg>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
    </animation>

    <animation>
        <object-name>Head</object-name>
        <object-name>StandIlluminator-spot-01</object-name>
        <object-name>StandIlluminator-spot-02</object-name>
        <type>rotate</type>
        <property>sim/G91/accessories/standIlluminators/pitch</property>
        <factor>1.0</factor>
        <offset-deg>0.0</offset-deg>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>1.95</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>lamp glass (Meshed)</object-name>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/G91/accessories/standIlluminators/light/intensity</property>
                <factor>0.1</factor>
                <min>0.0</min>
                <max>1.0</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>

    <!-- Compositor lights -->

    <animation>
        <type>select</type>
        <object-name>StandIlluminator-spot-01</object-name>
        <object-name>StandIlluminator-spot-02</object-name>
        <condition>
            <and>
                <greater-than>
                    <property>sim/G91/accessories/standIlluminators/light/intensity</property>
                    <value type="double">0.01</value>
                </greater-than>
                <equals>
                    <property>sim/G91/accessories/standIlluminators/light/active</property>
                    <value type="int">1</value>
                </equals>
            </and>
        </condition>
    </animation>

    <light>
        <name>StandIlluminator-spot-01</name>
        <type>spot</type>
        <position>
            <x-m>0.3</x-m>
            <y-m>0.0</y-m>
            <z-m>1.95</z-m>
        </position>
        <direction>
            <pitch-deg>-90.0</pitch-deg>
            <roll-deg>0.0</roll-deg>
            <heading-deg>0.0</heading-deg>
        </direction>
        <ambient>
            <r>0.0</r>
            <g>0.0</g>
            <b>0.0</b>
            <a>1</a>
        </ambient>
        <diffuse>
            <r>8.0</r>
            <g>7.0</g>
            <b>6.0</b>
            <a>1</a>
        </diffuse>
        <specular>
            <r>0.4</r>
            <g>0.3</g>
            <b>0.3</b>
            <a>1</a>
        </specular>
        <attenuation>
            <c>0.05</c>
            <l>0.05</l>
            <q>0.05</q>
        </attenuation>
        <spot-exponent>20</spot-exponent>
        <spot-cutoff>89</spot-cutoff>
        <range-m>100.0</range-m>
        <dim-factor>
            <property>sim/G91/accessories/standIlluminators/light/intensity</property>
        </dim-factor>
    </light>

    <light>
        <name>StandIlluminator-spot-02</name>
        <type>spot</type>
        <position>
            <x-m>1.0</x-m>
            <y-m>0.0</y-m>
            <z-m>1.95</z-m>
        </position>
        <direction>
            <pitch-deg>90.0</pitch-deg>
            <roll-deg>0.0</roll-deg>
            <heading-deg>0.0</heading-deg>
        </direction>
        <ambient>
            <r>0.0</r>
            <g>0.0</g>
            <b>0.0</b>
            <a>1</a>
        </ambient>
        <diffuse>
            <r>8.0</r>
            <g>7.0</g>
            <b>6.0</b>
            <a>1</a>
        </diffuse>
        <specular>
            <r>0.4</r>
            <g>0.3</g>
            <b>0.3</b>
            <a>1</a>
        </specular>
        <attenuation>
            <c>2.0</c>
            <l>0.01</l>
            <q>0.01</q>
        </attenuation>
        <spot-exponent>60</spot-exponent>
        <spot-cutoff>30</spot-cutoff>
        <range-m>0.4</range-m>
        <dim-factor>
            <property>sim/G91/accessories/standIlluminators/light/intensity-spot</property>
        </dim-factor>
    </light>


</PropertyList>
