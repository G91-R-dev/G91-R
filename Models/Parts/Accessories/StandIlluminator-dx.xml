    <animation>
        <type>blend</type>
        <property>sim/G91/glass_camera_reflections</property>
        <factor>0.2</factor>
        <object-name>lamp cone internal (Meshed)</object-name>
    </animation>

    <!-- Animations -->

    <animation>
        <type>knob</type>
        <object-name>pipe top (Meshed)</object-name>
        <object-name>plate top (Meshed)</object-name>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/G91/accessories/illuminators/dx/yaw</property>
                <factor>3.0</factor>
                <min>0</min>
                <max>360</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>support top (Meshed)</object-name>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/sim/G91/accessories/illuminators/dx/pitch</property>
                <factor>3.0</factor>
                <min>-10.0</min>
                <max>30.0</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <object-name>Head</object-name>
        <object-name>support top (Meshed)</object-name>
        <object-name>StandIlluminator-spot-01</object-name>
        <object-name>StandIlluminator-spot-02</object-name>
        <type>rotate</type>
        <property>sim/G91/accessories/illuminators/dx/yaw</property>
        <factor>1.0</factor>
        <offset-deg>0.0</offset-deg>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>0.0</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1</z>
        </axis>
    </animation>

    <animation>
        <object-name>Head</object-name>
        <object-name>StandIlluminator-spot-01</object-name>
        <object-name>StandIlluminator-spot-02</object-name>
        <type>rotate</type>
        <property>/sim/G91/accessories/illuminators/dx/pitch</property>
        <factor>1.0</factor>
        <offset-deg>0.0</offset-deg>
        <center>
            <x-m>0.0</x-m>
            <y-m>0.0</y-m>
            <z-m>1.95</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>lamp glass (Meshed)</object-name>
        <action>
            <binding>
                <command>property-adjust</command>
                <property>/sim/G91/accessories/illuminators/dx/intensity</property>
                <factor>0.1</factor>
                <min>0.0</min>
                <max>1.0</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>

    <!-- Compositor lights -->

    <model>
        <path>Aircraft/G91-R1B_HD/Models/Parts/Accessories/StandIlluminator-dx-compositor.xml</path>
        <condition>
            <and>
                <greater-than>
                    <property>/sim/G91/accessories/illuminators/dx/intensity</property>
                    <value type="double">0.01</value>
                </greater-than>
                <equals>
                    <property>/sim/G91/accessories/illuminators/dx/active</property>
                    <value type="int">1</value>
                </equals>
            </and>
        </condition>
    </model>


</PropertyList>
