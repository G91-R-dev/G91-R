        </transparency>
        <diffuse>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </diffuse>
        <ambient>
            <red>0.0</red>
            <green>0.0</green>
            <blue>0.0</blue>
        </ambient>
        <emission>
            <red>0.0</red>
            <green>0.0</green>
            <blue>0.0</blue>
            <factor>0.0</factor>
        </emission>
    </animation>

    <animation>
