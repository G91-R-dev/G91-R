    <!-- Emergency Canopy Open Sx -->
    
    <animation>
        <type>pick</type>
        <object-name>F_door_em_canopyDX.002</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
                <command>property-toggle</command>
                <property>sim/G91/switchs/canopy/EmergencyCanopyOpenSx</property>
                <min>0</min>
                <max>1</max>
            </binding>
            <binding>
                <command>property-interpolate</command>
                <property>sim/G91/switchs/canopy/EmergencyCanopyOpenSx_timing</property>
                <property>sim/G91/switchs/canopy/EmergencyCanopyOpenSx</property>
                <time>1</time>
            </binding>
            <binding>
                <command>nasal</command>
                <script>
                    <![CDATA[
                    var self = cmdarg();
                    var emer_Canopy_Jett_Sx = getprop("sim/G91/switchs/canopy/handle/Emer_Canopy_Jett_Sx");
                    var canopyOpenSx = getprop("sim/G91/switchs/canopy/EmergencyCanopyOpenSx");
                    if (emer_Canopy_Jett_Sx == 1 and canopyOpenSx == 0) {
                    setprop("sim/G91/switchs/canopy/handle/Emer_Canopy_Jett_Sx",0);
                    }
                    ]]>
                </script>
            </binding>
            <decrease>
                <binding>
                    <command>property-assign</command>
                    <property>/sim/G91/handle/Emer_Canopy_Jett_Sx</property>
                    <value>0</value>
                </binding>
            </decrease>
        </action>
    </animation>
    
