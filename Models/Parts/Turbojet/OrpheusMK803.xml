    <!-- Engine rotate blades -->
    
    <animation>
        <object-name>node11</object-name>
        <type>spin</type>
        <condition>
            <or>
                <property>/engines/engine[0]/running</property>
                <greater-than>		
                    <property>velocities/airspeed-kt</property>
                    <value>200</value>
                </greater-than>
            </or>
        </condition>
        <property>engines/engine/n1</property>
        <factor>25</factor>
        <center>
            <x-m>-0.8</x-m>
            <y-m>-0.30</y-m>
            <z-m>0.62</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    
