<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <path>strut_tank.ac</path>
    
    <!-- On internal Hardpoints -->
       
    <animation>
        <type>select</type>
        <object-name>strut_dx_ant</object-name>
        <object-name>strut_dx_post</object-name>
        <object-name>strut_sx_ant</object-name>
        <object-name>strut_sx_post</object-name>
        <condition>
            <or>
                <equals>
                    <property>sim/G91/stores/stationSxInternalTypeLoad</property>
                    <value>10</value>
                </equals>
                <equals>
                    <property>sim/G91/stores/stationDxInternalTypeLoad</property>
                    <value>10</value>
                </equals>
            </or>
        </condition>
    </animation>
    
    <animation> 
        <type>material</type>
        <object-name>strut_dx_ant</object-name>
        <object-name>strut_dx_post</object-name>
        <object-name>strut_sx_ant</object-name>
        <object-name>strut_sx_post</object-name>
        <texture>Pictures/G91-Dark-Grey.png</texture>
        <ambient>
            <red>0.8</red>
            <green>0.8</green>
            <blue>0.8</blue>
        </ambient>
    </animation>

</PropertyList>
