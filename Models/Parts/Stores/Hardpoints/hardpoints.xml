<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <path>upright.ac</path>
    
    <!-- internal Hardpoints -->
    
    <animation>
        <type>select</type>
        <object-name>upright_dx</object-name>
        <object-name>upright_sx</object-name>
        <condition>
            <or>
                <equals>
                    <property>sim/G91/stores/stationSxInternalTypeLoad</property>
                    <value>10</value>
                </equals>
                <equals>
                    <property>sim/G91/stores/stationDxInternalTypeLoad</property>
                    <value>10</value>
                </equals>
            </or>
        </condition>
    </animation>
    
    <model>
        <path>strut_tank.xml</path>
    </model>

</PropertyList>
