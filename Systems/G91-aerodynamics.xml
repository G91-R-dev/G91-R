                    </tableData>
                </table>
            </product>
        </function>
        
        <function name="aero/force/Drag_induced">
            <description>Induced drag</description>
            <product>
                <property>aero/qbar-psf</property>
                <property>metrics/Sw-sqft</property>
                <property>aero/cl-squared</property>
                <value>0.01</value>
            </product>
        </function>
        
        <function name="aero/force/Drag_mach">
            <description>Drag due to mach</description>
            <product>
                <property>aero/qbar-psf</property>
                <property>metrics/Sw-sqft</property>
                <table>
                    <independentVar lookup="row">velocities/mach</independentVar>
                    <tableData>
                        0.00  0.000
                        0.81  0.010
                        1.00  0.035
                        1.10  0.043
                        2.00  0.035
                    </tableData>
                </table>
            </product>
        </function>
        
        <function name="aero/force/Drag_flap">
            <description>Drag due to flaps</description>
            <product>
                <property>aero/qbar-psf</property>
                <property>metrics/Sw-sqft</property>
                <property>fcs/flap-pos-deg</property>
                <value> 0.00267 </value>
            </product>
        </function>
        
        <function name="aero/force/Drag_gear">
            <description>Drag due to gear</description>
            <product>
                <property>aero/qbar-psf</property>
                <property>metrics/Sw-sqft</property>
                <property>gear/gear-pos-norm</property>
                <value>0.01846</value>
            </product>
        </function>
        
        <function name="aero/force/Drag_beta">
            <description>Drag due to sideslip</description>
            <product>
                <property>aero/qbar-psf</property>
                <property>metrics/Sw-sqft</property>
                <table>
                    <independentVar lookup="row">aero/beta-rad</independentVar>
                    <tableData>
                        -1.57  1.230
                        -0.26  0.050
                        0.00  0.000
                        0.26  0.050
                        1.57  1.230
                    </tableData>
                </table>
            </product>
        </function>
        
        <function name="aero/force/Drag_elevator">
            <description>Drag due to Elevator Deflection</description>
            <product>
                <property>aero/qbar-psf</property>
                <property>metrics/Sw-sqft</property>
                <abs><property>fcs/elevator-pos-norm</property></abs>
                <value>0.03</value>
            </product>
        </function>
        
        <function name="aero/force/Drag_speedbrake">
            <description>Drag due to Speedbrake Deflection</description>
            <product>
                <property>aero/qbar-psf</property>
                <property>metrics/Sw-sqft</property>
                <abs><property>fcs/speedbrake-pos-norm</property></abs>
                <property>aero/constant/Drag_speedbrake-coefficient</property>
            </product>
        </function>
        
    </axis>
    
    <property value="-1.2146">aero/constant/Side_beta-coefficient</property>
    <property value="1.4227">aero/constant/Side_yaw-coefficient</property>
    <property value="0.3816">aero/constant/Side_rudder-coefficient</property>
    
