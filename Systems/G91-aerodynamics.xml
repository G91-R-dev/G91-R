            <table>
                <independentVar lookup="row">aero/h_b-mac-ft</independentVar>
                <tableData>
                    0.00  1.203
                    1.00  1.127
                    1.50  1.090
                    2.00  1.073
                    3.00  1.046
                    4.00  1.055
                    5.00  1.019
                    6.00  1.013
                    7.00  1.008
                    8.00  1.006
                    9.00  1.003
                    10.0  1.002
                    10.1  1.000
                </tableData>
            </table>
        </product>
    </function>
    
    <function name="aero/function/ground-effect-factor-drag">
        <description>Change in drag due to ground effect</description>
        <product>
            <v>1.0</v>
            <table>
                <independentVar lookup="row">aero/h_b-mac-ft</independentVar>
                <tableData>
                    0.00  0.480
                    1.00  0.515
                    1.50  0.629
                    2.00  0.709
                    3.00  0.815
                    4.00  0.882
