    <channel name="Bus 0 - Battery" execrate="8">
        
        <fcs_function name="systems/electric/bus[0]/I-To-Battery">
            <function>
                <ifthen>
                    <p>systems/manual-switches/cockpit/sw-bus0-bus1-battery</p>
                    <sum>
                        <p>systems/electric/bus[0]/I</p>
                        <p>systems/electric/bus[1]/I</p>
                        <p>systems/electric/bus[2]/I</p>
                    </sum>
                    <p>systems/electric/bus[0]/I</p>
                </ifthen>
            </function>
        </fcs_function>
        
        <fcs_function name="systems/electric/bus[0]/V">
            <function>
                <ifthen>
                    <p>systems/manual-switches/cockpit/sw-bus0-bus1-battery</p>
                    <ifthen>
                        <gt>
                            <p>systems/electric/bus[1]/V</p>
                            <p>systems/electric/bus[0]/battery-V</p>
                        </gt>
                        <p>systems/electric/bus[1]/V</p>
                        <p>systems/electric/bus[0]/battery-V</p>
                    </ifthen>
                    <p>systems/electric/bus[0]/battery-V</p>
                </ifthen>
            </function>
        </fcs_function>
        
        <fcs_function name="systems/electric/bus[0]/W-To-Battery">
            <function>
                <ifthen>
                    <p>systems/manual-switches/cockpit/sw-bus0-bus1-battery</p>
                    <ifthen>
                        <p>systems/electric/bus[1]/internal-power-generator-inverted-current-rele-on</p>
                        <product>
                            <v>250.0</v>
                            <quotient>
                                <difference>
                                    <p>systems/electric/bus[1]/internal-power-generator-V</p>
                                    <p>systems/electric/bus[0]/battery-V</p>
                                </difference>
                                <p>systems/electric/bus[1]/internal-power-generator-V</p>
                            </quotient>
                        </product>
                        <product>
                            <p>systems/electric/bus[0]/I-To-Battery</p>
                            <p>systems/electric/bus[0]/V</p>
                            <v>-1.0</v>
                        </product>
                    </ifthen>
                    <product>
                        <p>systems/electric/bus[0]/I-To-Battery</p>
                        <p>systems/electric/bus[0]/V</p>
                        <v>-1.0</v>
                    </product>
                </ifthen>
            </function>
        </fcs_function>
        
        <integrator name="systems/electric/bus[0]/battery-energy-change-J">
            <input>systems/electric/bus[0]/W-To-Battery</input>
            <c1>1.0</c1>
        </integrator>
        
        <summer name="systems/electric/bus[0]/battery-capacity-J">
            <input>systems/electric/bus[0]/battery-capacity-initial-J</input>
            <input>systems/electric/bus[0]/battery-energy-change-J</input>
            <clipto>
                <min>0.0</min>
                <max>systems/electric/bus[0]/battery-capacity-max-J</max>
            </clipto>
        </summer>
        
        <fcs_function name="systems/electric/bus[0]/battery-charge-norm">
            <function>
                <quotient>
                    <p>systems/electric/bus[0]/battery-capacity-J</p>
                    <p>systems/electric/bus[0]/battery-capacity-max-J</p>
                </quotient>
            </function>
        </fcs_function>
        
        <fcs_function name="systems/electric/bus[0]/battery-V">
            <function>
                <table>
                    <independentVar lookup="row">systems/electric/bus[0]/battery-charge-norm</independentVar>
                    <tableData>
                        0.0     0.0
                        0.02    21.0
                        0.1     22.5
                        0.2     23.0
                        0.5     24.0
                        0.8     24.8
                        0.9     25.0
                        1.0     25.4
                    </tableData>
                </table>
            </function>
        </fcs_function>
        
    </channel>
    
    
    <channel name="Bus Primary" execrate="8">
        
        <switch name="systems/electric/bus[1]/V">
            <default value="0"/>
            <test logic="AND" value="28.0">
                systems/electric/bus[1]/external-power-generator-on == 1
            </test>
            <test logic="AND" value="0">
                systems/manual-switches/cockpit/sw-bus0-bus1-battery == 0
                systems/electric/bus[1]/internal-power-generator-inverted-current-rele-on == 0
            </test>  
            <test logic="AND" value="systems/electric/bus[0]/battery-V">
                systems/manual-switches/cockpit/sw-bus0-bus1-battery == 1
                systems/electric/bus[1]/internal-power-generator-inverted-current-rele-on == 0
            </test>
            <test logic="AND" value="systems/electric/bus[1]/internal-power-generator-V">
                systems/manual-switches/cockpit/sw-bus0-bus1-battery == 0
                systems/electric/bus[1]/internal-power-generator-inverted-current-rele-on == 1
            </test>
            <test logic="AND" value="systems/electric/bus[1]/internal-power-generator-V">
                systems/manual-switches/cockpit/sw-bus0-bus1-battery == 1
                systems/electric/bus[1]/internal-power-generator-inverted-current-rele-on == 1
            </test> 
        </switch>
        
        <switch name="systems/electric/bus[1]/internal-power-generator-inverted-current-rele-on">
            <default value="0"/>
            <test logic="AND" value="0">
                systems/electric/bus[1]/internal-power-generator-V LT 18
            </test>
            <test logic="AND" value="1">
                systems/manual-switches/cockpit/sw-internal-power-generator == 1
                systems/electric/bus[1]/internal-power-generator-V GE 18
                <test logic="OR">
                    <test logic="AND">
                        systems/manual-switches/cockpit/sw-bus0-bus1-battery == 0
                    </test>
                    <test logic="AND">
                        systems/manual-switches/cockpit/sw-bus0-bus1-battery == 1
                        systems/electric/bus[1]/internal-power-generator-V GT systems/electric/bus[0]/battery-V
                    </test>
                </test>
            </test>
        </switch>
        
        <switch name="systems/electric/bus[1]/internal-power-generator-I">
            <default value="0"/>
            <test logic="AND" value="systems/electric/bus[1]/internal-power-generator-I-Allload">
                systems/electric/bus[1]/internal-power-generator-inverted-current-rele-on == 1
            </test>
        </switch>
        
        <pure_gain name="systems/electric/bus[1]/internal-power-generator-W-All-Load">
            <input>systems/electric/bus[1]/internal-power-generator-I-Allload</input>
            <gain>systems/electric/bus[1]/V</gain>
        </pure_gain>
        
        <fcs_function name="systems/electric/bus[1]/internal-power-generator-I-Fiels">
            <function>
                <ifthen>
                    <p>systems/manual-switches/cockpit/sw-internal-power-generator</p>
                    <quotient>
                        <p>systems/electric/bus[1]/V</p>
                        <v>2.6</v>
                    </quotient>
                    <v>0.0</v>
                </ifthen>
            </function>
        </fcs_function>
        
        <fcs_function name="systems/electric/bus[1]/internal-power-generator-Vout-Potenzial">
            <function>
                <ifthen>
                    <ifthen>
                        <gt>
                            <p>systems/electric/bus[1]/V</p>
                            <v>18.0</v>
                        </gt>
                        <p>systems/manual-switches/cockpit/sw-internal-power-generator</p>
                        <v>0</v>
                    </ifthen>
                    <product>
                        <table>
                            <independentVar>propulsion/engine/n1</independentVar>
                            <tableData>
                                0.0   0.0
                                10.0  12.0
                                20.0  18.0
                                35.0  28.0
                                100.0 30.0
                            </tableData>
                        </table>
                        <table>
                            <independentVar>systems/electric/bus[1]/internal-power-generator-I</independentVar>
                            <tableData>
                                0.0    1.0
                                200.0  1.0
                                300.0  0.9
                                400.0  0.6
                                1000.0 0.0
                            </tableData>
                        </table>
                    </product>
                    <v>0.0</v>
                </ifthen>
            </function>
        </fcs_function>
        
        <lag_filter name="systems/electric/bus[1]/internal-power-generator-V">
            <input>systems/electric/bus[1]/internal-power-generator-Vout-Potenzial</input>
            <c1>1.0</c1>
        </lag_filter>
        
        <summer name="systems/electric/bus[1]/internal-power-generator-I-Allload">
            <input>systems/electric/bus[1]/I-bus0-to-bus1</input>
            <input>systems/electric/bus[1]/I</input>
            <input>systems/electric/bus[2]/I</input>
        </summer>
        
        <switch name="systems/electric/bus[1]/I-bus0-to-bus1">
            <default value="0"/>
            <test logic="AND" value="systems/electric/bus[0]/I">
                systems/manual-switches/cockpit/sw-bus0-bus1-battery == 1
            </test>
        </switch>
        
        <switch name="systems/electric/bus-link/relay-bus1-bus2-secondary">
            <default value="0"/>
            <test logic="OR" value="1">
                systems/electric/bus[1]/internal-power-generator-inverted-current-rele-on == 1
            </test>
        </switch>
        
        <pure_gain name="systems/electric/bus[1]/W">
            <input>systems/electric/bus[1]/I</input>
            <gain>systems/electric/bus[1]/V</gain>
        </pure_gain>
        
    </channel>
    
    
    <channel name="Bus Secondary" execrate="8">
        
        <switch name="systems/electric/bus[2]/V">
            <default value="0"/>
            <test logic="AND" value="systems/electric/bus[1]/V">
                systems/electric/bus-link/relay-bus1-bus2-secondary == 1
            </test>
        </switch>
        
        <pure_gain name="systems/electric/bus[2]/W">
            <input>systems/electric/bus[2]/I</input>
            <gain>systems/electric/bus[2]/V</gain>
        </pure_gain>
        
    </channel>
    
    
    <channel name="Inverters" execrate="32">
        
        <!-- primary inverter -->
        
        <switch name="systems/electric/inv-primary/active">
            <default value="0"/>
            <test logic="AND" value="115">
                systems/electric/bus[2]/primary-inverter/fuse == 1
                systems/manual-switches/cockpit/sw-primary-inverter-on == 1
                systems/electric/bus[2]/V GE 26
                systems/electric/inv-primary/emergency-on-bus1 == 0
            </test>
            <test logic="AND" value="115">
                systems/electric/bus[2]/primary-inverter/fuse == 1
                systems/manual-switches/cockpit/sw-primary-inverter-on == 1
                systems/electric/bus[2]/V LT 26
                systems/electric/inv-primary/emergency-on-bus1 == 1
            </test>
        </switch>
        
        <lag_filter name="systems/electric/inv-primary/V">
            <input>systems/electric/inv-primary/active</input>
            <c1>0.5</c1>
        </lag_filter>
        
        <switch name="systems/electric/bus[2]/primary-inverter/I">
            <default value="0"/>
            <test logic="AND" value="systems/electric/inv-primary/I">
                systems/electric/inv-primary/emergency-on-bus1 == 0
                systems/electric/inv-primary/active GE 10
            </test>
        </switch>
        
        <switch name="systems/electric/bus[1]/primary-inverter/I">
            <default value="0"/>
            <test logic="AND" value="systems/electric/inv-primary/I">
                systems/electric/inv-primary/emergency-on-bus1 == 1
                systems/electric/inv-primary/active GE 10
            </test>
        </switch>
        
        <summer name="systems/electric/inv-primary/I">
            <input>systems/electric/inv-primary/I0</input>
            <input>systems/electric/bus[1]/turn-indicator/I</input>
            <input>systems/electric/bus[1]/attitude-indicator/I</input>
            <input>systems/electric/inv-primary/cockpit-air-temp/I</input>
        </summer>
        
        <!-- Emergency primary inverter on Bus1 -->
        
        <switch name="systems/electric/inv-primary/emergency-on-bus1">
            <default value="0"/>
            <test logic="AND" value="1">
                systems/electric/bus[2]/primary-inverter/fuse == 1
                systems/manual-switches/cockpit/sw-primary-inverter-on == 1
                systems/electric/bus[2]/V LT 26
                systems/electric/bus[1]/V GE 26
            </test>
        </switch>
        
        <!-- Secondary inverter -->
        
        <switch name="systems/electric/inv-secondary/active">
            <default value="0"/>
            <test logic="AND" value="115">
                systems/electric/bus[1]/secondary-inverter/fuse == 1
                systems/manual-switches/cockpit/sw-secondary-inverter-on == 1
                systems/electric/bus[1]/V GE 26
            </test>
        </switch>
        
        <switch name="systems/electric/bus[1]/secondary-inverter/I">
            <default value="0"/>
            <test logic="AND" value="systems/electric/inv-secondary/I">
                systems/electric/inv-secondary/active GE 10
            </test>
        </switch>
        
        <summer name="systems/electric/inv-secondary/I">
            <input>systems/electric/inv-secondary/I0</input>
        </summer>
        
        <lag_filter name="systems/electric/inv-secondary/V">
            <input>systems/electric/inv-secondary/active</input>
            <c1>0.5</c1>
        </lag_filter>
        
    </channel>
    
    
</system>

