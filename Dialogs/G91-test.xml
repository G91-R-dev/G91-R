<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <name>G91_test_dialog</name>
    <layout>vbox</layout>
    <default-padding>7</default-padding>
    <modal>false</modal>
    <width>1000</width>
    
    <!-- title bar -->
    <group>
        <layout>hbox</layout>
        <empty><stretch>true</stretch></empty>
        <text>
            <label>Testing G91</label>
        </text>
        <empty><stretch>true</stretch></empty>
        <button>
            <legend/>
            <key>Esc</key>
            <pref-width>16</pref-width>
            <pref-height>16</pref-height>
            <border>2</border>
            <binding>
                <command>dialog-close</command>
            </binding>
        </button>
    </group>
    
    <hrule/>
    
    <group>
        <layout>hbox</layout>
        <group>
            <layout>table</layout>
            <text>
                <label>Log for debug active</label>
                <halign>right</halign>
                <row>0</row>
                <col>0</col>
            </text>
            <slider>
                <row>0</row>
                <col>1</col>
                <name>Testing_log</name>
                <property>sim/G91/testing/log</property>
                <min>0</min>
                <max>2</max>
                <step>1</step>  
                <binding>
                    <command>dialog-apply</command>
                </binding>
            </slider>
            <text>
                <row>0</row>
                <col>2</col>
                <label>999999</label>
                <live>true</live>
                <format>%1.0f</format>
                <property>sim/G91/testing/log</property>
                <color>
                    <red>0.9</red>
                    <green>0.9</green>
                    <blue>0.9</blue>
                </color>
            </text>
        </group>
    </group>
    
