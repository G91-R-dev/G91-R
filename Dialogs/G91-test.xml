<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
    
    <name>G91_test_dialog</name>
    <layout>vbox</layout>
    <default-padding>7</default-padding>
    <modal>false</modal>
    <width>700</width>
    
    <!-- title bar -->
    <group>
        <layout>hbox</layout>
        <empty><stretch>true</stretch></empty>
        <text>
            <label>Testing G91</label>
        </text>
        <empty><stretch>true</stretch></empty>
        <button>
            <legend/>
            <key>Esc</key>
            <pref-width>16</pref-width>
            <pref-height>16</pref-height>
            <border>2</border>
            <binding>
                <command>dialog-close</command>
            </binding>
        </button>
    </group>
    
    <hrule/>
    
    <!-- Hydraulic pressure test -->
    <group>
        <layout>hbox</layout>
        <text>
            <halign>left</halign>
            <label>Hydraulic pressure (psi) [Flow in|out]</label>
        </text>
        <group>
            <layout>hbox</layout>
            <halign>right</halign>
            <text>
                <label>99999</label>
                <live>true</live>
                <format>%5.1f </format>
                <property>fdm/jsbsim/systems/hydraulics/flight-system-psi</property>
                <color>
                    <red>0.9</red>
                    <green>0.9</green>
                    <blue>0.9</blue>
                </color>
            </text>
            <text>
                <label>999999</label>
                <live>true</live>
                <format>[%4.2f|</format>
                <property>fdm/jsbsim/systems/hydraulics/pump_flow_rate</property>
                <color>
                    <red>0.9</red>
                    <green>0.9</green>
                    <blue>0.9</blue>
                </color>
            </text>
            <text>
                <label>999999</label>
                <live>true</live>
                <format>%4.2f]</format>
                <property>fdm/jsbsim/systems/hydraulics/lost_flow_rate</property>
                <color>
                    <red>0.9</red>
                    <green>0.9</green>
                    <blue>0.9</blue>
                </color>
            </text>
            
            <empty>
                <stretch>true</stretch>
            </empty>
            
            <vrule/>
            <group>
                <width>100</width>
                <text>
                    <label> 0.01</label>
                </text>
            </group>
            <slider>
                <name>hydraulic_pressure_loss</name>
                <property>fdm/jsbsim/systems/hydraulics/lost-performance-system</property>
                <min>0.1</min>
                <max>1.0</max>
                <step>0.01</step>  
                <binding>
                    <command>dialog-apply</command>
                </binding>
            </slider>
            <group>
                <width>30</width>      
                <text>
                    <label>1</label>
                </text>
            </group>
            <text>
                <label>9999</label>
                <live>true</live>
                <format>(%2.2f)</format>
                <property>fdm/jsbsim/systems/hydraulics/lost-performance-system</property>
                <color>
                    <red>0.2</red>
                    <green>0.9</green>
                    <blue>0.2</blue>
                </color>
            </text>
        </group>
    </group>
    
