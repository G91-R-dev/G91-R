                <min>0</min>
                <max>6</max>
                <step>1</step>  
                <binding>
                    <command>dialog-apply</command>
                </binding>
            </slider>
            <text>
                <row>0</row>
                <col>2</col>
                <label>MMMMMMMMMMMMMMMMMMMM</label>
